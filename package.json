{
  "name": "@tanker/user-token",
  "version": "0.0.2",
  "description": "User token generator for tanker.io",
  "author": "Tanker Team",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "README.md"
  ],
  "scripts": {
    "build:es": "babel -d es src",
    "build:babel": "BABEL_ENV=es5 babel -d lib src",
